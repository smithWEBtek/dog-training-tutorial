<%= form_for @tutorial do |f| %>
<%# form_for knows that an empty, unsaved model object needs a new form and a populated object needs an edit form %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>

  <%= f.label :content %><br>
  <%= f.text_area :content %><br>

  <%= f.submit %>
<% end %>
